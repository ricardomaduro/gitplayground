  "scripts": {
    "test": "jest --no-cache --coverage --verbose",
    "start": "set NODE_ENV=development&& nodemon",
    "babel-node": "node_modules/.bin/babel-node -- server.js",
    "production-install": "node service/service_install.js",
    "production-uninstall": "node service/service_uninstall.js",
    "build-production": "set NODE_ENV=production&& babel server.js config.js routes.js -d build && webpack -p --progress --colors",
    "production": "set NODE_ENV=production&& forever start build/server.js",
    "test-production": "set NODE_ENV=production&& node build/server.js",
    "postinstall": "node post-install.js",
    "build-api": "set NODE_ENV=production&&node_modules\\.bin\\tsc ./app/cproxy/index.js --allowJs --outDir ./build/cproxy --target \"es5\" --lib \"es6,dom\"",
    "produce-brand": "set NODE_ENV=production&& webpack --progress -p --config webpack.producebrand.config.js",
    "scaffold-brand": "node scaffold-whitelabel.js",
    "make-brand": "set NODE_ENV=production&& babel server.js config.js routes.js -d build&& npm run produce-brand&& npm run scaffold-brand"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "typescript-babel-jest"
    },
