pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sh '''
                    rm -rf gitplayground
                    git clone https://github.com/ricardomaduro/gitplayground.git
                    pwd
                    cd gitplayground
                    pwd
                    ls -l
                '''
                echo 'Done'
            }
        }
    }
}
